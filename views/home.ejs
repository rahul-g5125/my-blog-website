<%- include("partials/header") %> 

<h1>Home</h1>
<p> <%= homeStartContent %>  </p>

<% posts.forEach(element => { %>
    <h3> <%= element.title %>  </h3>
    <% const trimmedString = element.body.substring(0, 101); %>
    <%= trimmedString + "..." %>
    <a href="/posts/<%= element.title %> "> Read More</a>
<% }) %>

<%- include("partials/footer") %> 